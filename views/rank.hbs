<!DOCTYPE html>

<html>
  {{> header}}
	<body>
		<h1> Rank </h1>
    {{#if Msg }}
      <h2>{{Msg}}</h2>
    {{/if}}
    <table class="table" style="width:50%">
      <tr>
          <th>First name</th>
          <th>Last Name</th> 
          <th>Win Rate</th>
      </tr>
    {{#each Table}}
      <div class="row">
        <tr>
          <th>{{this.first_name}}</th>
          <th>{{this.last_name}}</th> 
          <th>{{this.win_rate}}%</th>
         </tr>
      </div>
    {{/each}}
    </table>
    <div>
      <a href="/"><button class="btn btn-default" aria-label="Left Align"> Back </button></a>
    </div>
    

		{{> footer}}
	</body>
</html>